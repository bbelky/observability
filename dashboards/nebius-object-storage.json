{
  "uid": "nebius-object-storage",
  "title": "Nebius Object Storage",
  "description": "Nebius Object Storage Overview.",
  "tags": [
    "Nebius",
    "Object Storage"
  ],
  "timezone": "browser",
  "editable": false,
  "graphTooltip": 0,
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "fiscalYearStartMonth": 0,
  "refresh": "1m",
  "schemaVersion": 41,
  "panels": [
    {
      "type": "row",
      "collapsed": false,
      "title": "Storage space",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 0,
      "panels": []
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by(bucket) (rate(http_bytes_sent{bucket=~\"$bucket\"}[$__rate_interval])) OR on() vector(0)",
          "legendFormat": "Download {{bucket}}",
          "refId": "A"
        },
        {
          "expr": "sum by(bucket) (rate(http_bytes_received{bucket=~\"$bucket\"}[$__rate_interval])) OR on() vector(0)",
          "legendFormat": "Upload {{bucket}}",
          "refId": "B"
        }
      ],
      "title": "Traffic",
      "description": "Data transfer speed to and from storage.",
      "transparent": false,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 9,
        "w": 8,
        "x": 0,
        "y": 1
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true,
          "calcs": []
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "binBps",
          "thresholds": {
            "mode": "",
            "steps": []
          },
          "custom": {
            "fillOpacity": 5,
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Download "
            },
            "properties": [
              {
                "id": "custom.hideFrom",
                "value": {
                  "legend": true,
                  "tooltip": true,
                  "viz": false
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Upload "
            },
            "properties": [
              {
                "id": "custom.hideFrom",
                "value": {
                  "legend": true,
                  "tooltip": true,
                  "viz": true
                }
              }
            ]
          }
        ]
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by(bucket) (max by(bucket, counter) (last_over_time(buckets_stat_size{bucket=~\"$bucket\"}[1m])))",
          "legendFormat": "{{bucket}}",
          "refId": "A"
        }
      ],
      "title": "Total bucket size",
      "description": "Storage space used by all objects in a bucket.",
      "transparent": false,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 9,
        "w": 8,
        "x": 8,
        "y": 1
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true,
          "calcs": []
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "mode": "",
            "steps": []
          },
          "custom": {
            "fillOpacity": 70,
            "showPoints": "never"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by(bucket) (max by(bucket, counter) (last_over_time(buckets_stat_size{bucket=~\"$bucket\",storage_class=\"STANDARD\"}[1m])))",
          "legendFormat": "{{bucket}} Standard",
          "refId": "A"
        },
        {
          "expr": "sum by(bucket) (max by(bucket, counter) (last_over_time(buckets_stat_size{bucket=~\"$bucket\",storage_class=\"ENHANCED_THROUGHPUT\"}[1m])))",
          "legendFormat": "{{bucket}} Enhanced Throughput",
          "refId": "B"
        }
      ],
      "title": "Space by storage class",
      "description": "Amount of storage used by objects in different storage classes.",
      "transparent": false,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 9,
        "w": 8,
        "x": 16,
        "y": 1
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true,
          "calcs": []
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "mode": "",
            "steps": []
          },
          "custom": {
            "fillOpacity": 70,
            "showPoints": "never"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "collapsed": false,
      "title": "Requests for $bucket",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 10
      },
      "id": 0,
      "panels": [],
      "repeat": "bucket"
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by(handler) (rate(request_rate{bucket=\"$bucket\", operation_type=\"read\"}[$__rate_interval]))",
          "legendFormat": "{{handler}}",
          "refId": "A"
        }
      ],
      "title": "Read requests",
      "description": "Number of requests made to retrieve object content from a bucket. ",
      "transparent": false,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 11
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "mode": "",
            "steps": []
          },
          "custom": {
            "fillOpacity": 5,
            "showPoints": "never"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by(handler) (rate(request_rate{bucket=\"$bucket\", operation_type=\"mutate\"}[$__rate_interval]))",
          "legendFormat": "{{handler}}",
          "refId": "A"
        }
      ],
      "title": "Modify requests",
      "description": "Number of requests made to upload objects or modify object content. ",
      "transparent": false,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 11
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "mode": "",
            "steps": []
          },
          "custom": {
            "fillOpacity": 5,
            "showPoints": "never"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by(handler, http_code, api_error_code) (increase(http_errors_total{bucket=\"$bucket\"}[5m]))",
          "legendFormat": "{{handler}}:{{http_code}}:{{api_error_code}}",
          "refId": "A"
        }
      ],
      "title": "API errors",
      "description": "Number of errors when accessing S3 API. Number of errors per 5 minutes.",
      "transparent": false,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 11
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "",
            "steps": []
          },
          "custom": {
            "fillOpacity": 5,
            "showPoints": "never"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "row",
      "collapsed": false,
      "title": "Objects statistics",
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 19
      },
      "id": 0,
      "panels": []
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by(bucket) (max by(bucket, counter) (last_over_time(buckets_stat_quantity{bucket=~\"$bucket\", counter=\"simple_objects\"}[1m])))",
          "legendFormat": "{{bucket}} Simple objects",
          "refId": "A"
        },
        {
          "expr": "sum by(bucket) (max by(bucket, counter) (last_over_time(buckets_stat_quantity{bucket=~\"$bucket\", counter=\"multipart_objects\"}[1m])))",
          "legendFormat": "{{bucket}} Multipart objects",
          "refId": "B"
        },
        {
          "expr": "sum by(bucket) (max by(bucket, counter) (last_over_time(buckets_stat_quantity{bucket=~\"$bucket\", counter=\"inflight_parts\"}[1m])))",
          "legendFormat": "{{bucket}} Multipart uploads",
          "refId": "C"
        }
      ],
      "title": "Object counts",
      "description": "Number of objects. Single, multipart objects, and incomplete multipart uploads are counted separately.",
      "transparent": false,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 0,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "",
            "steps": []
          },
          "custom": {
            "fillOpacity": 5,
            "showPoints": "never"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by(bucket) (max by(bucket, counter) (last_over_time(buckets_stat_size{bucket=~\"$bucket\", counter=\"simple_objects\"}[1m])))",
          "legendFormat": "{{bucket}} Simple objects",
          "refId": "A"
        },
        {
          "expr": "sum by(bucket) (max by(bucket, counter) (last_over_time(buckets_stat_size{bucket=~\"$bucket\", counter=\"multipart_objects\"}[1m])))",
          "legendFormat": "{{bucket}} Multipart objects",
          "refId": "B"
        },
        {
          "expr": "sum by(bucket) (max by(bucket, counter) (last_over_time(buckets_stat_size{bucket=~\"$bucket\", counter=\"inflight_parts\"}[1m])))",
          "legendFormat": "{{bucket}} Multipart uploads",
          "refId": "C"
        }
      ],
      "title": "Space by object type",
      "description": "Amount of storage used by objects of different types.",
      "transparent": false,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 8,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "mode": "",
            "steps": []
          },
          "custom": {
            "fillOpacity": 70,
            "showPoints": "never"
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by(bucket) (max by(bucket, counter) (last_over_time(buckets_stat_quantity{bucket=~\"$bucket\", storage_class=\"STANDARD\"}[1m])))",
          "legendFormat": "{{bucket}} Standard",
          "refId": "A"
        },
        {
          "expr": "sum by(bucket) (max by(bucket, counter) (last_over_time(buckets_stat_quantity{bucket=~\"$bucket\", storage_class=\"ENHANCED_THROUGHPUT\"}[1m])))",
          "legendFormat": "{{bucket}} Enhanced Throughput",
          "refId": "B"
        }
      ],
      "title": "Object counts by storage class",
      "description": "Number of objects stored in different storage classes.",
      "transparent": false,
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 8,
        "x": 16,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "",
            "steps": []
          },
          "custom": {
            "fillOpacity": 5,
            "showPoints": "never"
          }
        },
        "overrides": []
      }
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "datasource",
        "skipUrlSync": false,
        "query": "prometheus",
        "current": {
          "text": "Nebius Services",
          "value": "Nebius Services"
        },
        "multi": false,
        "allowCustomValue": false,
        "includeAll": false,
        "auto": false,
        "auto_min": "10s",
        "auto_count": 30
      },
      {
        "type": "query",
        "name": "bucket",
        "skipUrlSync": false,
        "query": "label_values(buckets_stat_quantity, bucket)",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "multi": true,
        "allowCustomValue": false,
        "includeAll": true,
        "allValue": ".*",
        "auto": false,
        "auto_min": "10s",
        "auto_count": 30
      }
    ]
  },
  "annotations": {},
  "links": [
    {
      "title": "Docs",
      "type": "link",
      "icon": "doc",
      "tooltip": "",
      "url": "https://docs.nebius.com/object-storage",
      "tags": [],
      "asDropdown": false,
      "targetBlank": true,
      "includeVars": false,
